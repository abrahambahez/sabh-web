---
import { getCollection } from 'astro:content';

const posts = await getCollection('posts'); // Get posts

const collator = new Intl.Collator(undefined) // Sort entries
const sortedPosts = posts.sort((a,b) => collator.compare(a.data.createdAt, b.data.createdAt)).reverse()


// const { entry } = Astro.props;
const { Content } = await sortedPosts[0].render();

