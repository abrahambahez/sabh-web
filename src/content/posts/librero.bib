@article{martinezespinola&alvarado2022,
  title = {{Entre coaliciones, articulaciones y difracciones: hacia una pedagog\'ia feminista}},
  shorttitle = {{Entre coaliciones, articulaciones y difracciones}},
  author = {Martinez Esp{\'i}nola, Mar{\'i}a Victoria and Alvarado, Mariana},
  year = {2022},
  month = mar,
  publisher = {{Universidad Aut\'onoma de Manizales}},
  issn = {2248-6941},
  doi = {10.30854/anf.v29.n52.2022.825},
  urldate = {2023-05-09},
  abstract = {Objetivo: este escrito busca dar cuenta del camino experienciado desde ciertas claves epist\'emico-metodol\'ogicas, y a partir de ciertas pr\'acticas situadas en/desde los feminismos del Sur. Toma como punto de partida acciones de escrache hacia varones cis como enclave del an\'alisis de sus testimonios para dar cuenta de los m\'ultiples espacio-tiempos que fuimos precisando durante la investigaci\'on, a fin de dar materialidad a una epistemolog\'ia de las coaliciones; en la que la traducci\'on, la narrativa ficcionada y el pensar en conversaci\'on una junta a les otres, resultaron los dispositivos centrales. Metodolog\'ia: como eje de esta metodolog\'ia, las narrativas ficcionadas, a partir de testimonios propiciaron conversaciones, discusiones, debates, coaliciones y difractaron una elaboraci\'on colectiva de materiales que habilita a pensar estrategias pedag\'ogicas en clave feminista. Algunas de las preguntas de indagaci\'on que atraviesan y se traman en este escrito refieren a: \textquestiondown cu\'al ha sido la renovaci\'on metodol\'ogica que propician los feminismos del sur?, \textquestiondown qu\'e tensiones, conflictos, emergencias, cegueras y sorderas epistemol\'ogicas visibilizan los feminismos del sur?, \textquestiondown en qu\'e medida el desplazamiento sujeto-objeto a sujeta-sujetxs propicia otras formas de producci\'on de conocimiento?, \textquestiondown cu\'ales riesgos corremos si incluimos a nuevas masculinidades como informantes para pensar los escraches?, \textquestiondown cu\'ales son las variaciones que habilita la narrativa ficcionada?, \textquestiondown por qu\'e su potencia productiva nos corre de la representaci\'on a la difracci\'on?, \textquestiondown en qu\'e momento asumimos una pedagog\'ia del deseo como anclaje creativo?, \textquestiondown por qu\'e esa pedagog\'ia desbordar\'ia los canales conocidos de toda pedagog\'ia posible?, \textquestiondown c\'omo esas difracciones posibilitar\'ian una epistemolog\'ia de las coaliciones? Resultados: en el marco de este itinerar devino una pedagog\'ia feminista sustentada en una epistemolog\'ia de la localizaci\'on, del posicionamiento y de las articulaciones.},
  copyright = {info:eu-repo/semantics/openAccess},
  langid = {spanish},
  annotation = {Accepted: 2023-02-03T16:20:04Z},
  file = {/Users/sabhz/Library/Mobile Documents/com~apple~CloudDocs/Documentos/archivo/src/librero/martinezespinola&alvarado2022.pdf}
}

