}

@article{morente2018,
  title = {{De la acumulaci\'on a la apropiaci\'on: una reflexi\'on acerca del espacio p\'ublico en la ciudad contempor\'anea}},
  shorttitle = {{De la acumulaci\'on a la apropiaci\'on}},
  author = {Morente, Fran},
  year = {2018},
